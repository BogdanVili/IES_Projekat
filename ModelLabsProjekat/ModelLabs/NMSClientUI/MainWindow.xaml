<Window x:Class="NMSClientUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NMSClientUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1000">
    <Grid>
        <DockPanel>
            <ToolBar DockPanel.Dock="Top">
                <Button Content="Get Values" Click="GetValuesView"/>
                <Button Content="Get Extent Values" Click="GetExtentValuesView"/>
                <Button Content="Get Related Values" Click="GetRelatedValuesView"/>
            </ToolBar>

            <ContentControl x:Name="ContentView"/>
        </DockPanel>
    </Grid>
</Window>
